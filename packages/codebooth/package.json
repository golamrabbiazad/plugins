{
  "name": "@lqv/codebooth",
  "version": "0.0.2",
  "description": "GUI for interactive coding tutorials with CodeMirror",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./interpreters/*": {
      "import": "./dist/esm/interpreters/*.mjs",
      "require": "./dist/cjs/interpreters/*.cjs"
    },
    "./*": {
      "import": "./dist/esm/presets/*.mjs",
      "require": "./dist/cjs/presets/*.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/types/index.d.ts"
      ],
      "interpreters/*": [
        "./dist/types/interpteters/*.d.ts"
      ],
      "*": [
        "./dist/types/presets/*.d.ts"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liqvidjs/plugins.git"
  },
  "scripts": {
    "build": "pnpm build:clean; pnpm build:css; pnpm build:js; pnpm build:postclean",
    "build:clean": "rm -fr dist",
    "build:css": "stylus -o dist/style.css styl/style.styl; stylus -c -o dist/style.min.css styl/style.styl",
    "build:js": "tsc --module commonjs --outDir dist/cjs; tsc; node ../../build.mjs",
    "build:postclean": "rm -fr dist/tsconfig.tsbuildinfo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yuri Sulyma <yuri@liqvidjs.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liqvidjs/plugins/issues"
  },
  "homepage": "https://github.com/liqvidjs/plugins/tree/main/packages/codebooth",
  "devDependencies": {
    "@codemirror/autocomplete": "^0.19.15",
    "@codemirror/closebrackets": "^0.19.2",
    "@codemirror/commands": "^0.19.8",
    "@codemirror/comment": "^0.19.1",
    "@codemirror/fold": "^0.19.3",
    "@codemirror/gutter": "^0.19.9",
    "@codemirror/highlight": "^0.19.8",
    "@codemirror/history": "^0.19.2",
    "@codemirror/lang-css": "^0.19.3",
    "@codemirror/lang-html": "^0.19.4",
    "@codemirror/lang-javascript": "^0.19.7",
    "@codemirror/lang-python": "^0.19.5",
    "@codemirror/language": "^0.19.10",
    "@codemirror/lint": "^0.19.6",
    "@codemirror/matchbrackets": "^0.19.4",
    "@codemirror/rectangular-selection": "^0.19.2",
    "@codemirror/search": "^0.19.10",
    "@codemirror/state": "^0.19.9",
    "@codemirror/view": "^0.19.48",
    "@liqvid/recording": "^0.0.5",
    "@lqv/codemirror": "workspace:^",
    "@lqv/playback": "workspace:^",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsc-esm-fix": "^2.12.4"
  },
  "peerDependencies": {
    "@codemirror/autocomplete": ">=0.19.0",
    "@codemirror/closebrackets": ">=0.19.0",
    "@codemirror/commands": ">=0.19.0",
    "@codemirror/comment": ">=0.19.0",
    "@codemirror/fold": ">=0.19.0",
    "@codemirror/gutter": ">=0.19.0",
    "@codemirror/highlight": ">=0.19.0",
    "@codemirror/history": ">=0.19.0",
    "@codemirror/lang-css": ">=0.19.0",
    "@codemirror/lang-html": ">=0.19.0",
    "@codemirror/lang-javascript": ">=0.19.0",
    "@codemirror/lang-python": ">=0.19.0",
    "@codemirror/language": ">=0.19.0",
    "@codemirror/lint": ">=0.19.0",
    "@codemirror/matchbrackets": ">=0.19.0",
    "@codemirror/rectangular-selection": ">=0.19.0",
    "@codemirror/search": ">=0.19.0",
    "@codemirror/state": ">=0.19.0",
    "@codemirror/view": ">=0.19.0",
    "@liqvid/recording": "^0.0.5",
    "@lqv/codemirror": "workspace:^",
    "@lqv/playback": "workspace:^",
    "react": "^17.0.2"
  },
  "peerDependenciesMeta": {
    "@codemirror/lang-css": {
      "optional": true
    },
    "@codemirror/lang-html": {
      "optional": true
    },
    "@codemirror/lang-javascript": {
      "optional": true
    },
    "@codemirror/lang-python": {
      "optional": true
    },
    "@codemirror/lang-sql": {
      "optional": true
    }
  },
  "dependencies": {
    "@liqvid/keymap": "^1.1.3",
    "@liqvid/utils": "^1.6.0",
    "zustand": ">=4.0.0-rc.1"
  },
  "type": "module"
}
