{
  "name": "@lqv/remotion",
  "version": "0.0.1",
  "description": "Remotion compatibility layer for Liqvid plugins",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "pnpm build:clean; pnpm build:js; pnpm build:postclean",
    "build:clean": "rm -rf dist",
    "build:js": "tsc",
    "build:postclean": "find dist -name tsconfig.tsbuildinfo -delete",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yuri Sulyma <yuri@liqvidjs.org>",
  "license": "MIT",
  "dependencies": {
    "@liqvid/utils": "^1.6.3"
  },
  "devDependencies": {
    "@lqv/playback": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remotion": "^3.1.2"
  },
  "peerDependencies": {
    "@lqv/playback": "workspace:^",
    "react": ">=18.1.0",
    "remotion": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "remotion": {
      "optional": true
    }
  },
  "type": "module"
}